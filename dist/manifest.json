{
  "manifest_version": 3,
  "name": "Trello Booster",
  "version": "1.0.0",
  "action": {
    "default_popup": "src/popup.html"
  },
  "permissions": [
    "storage",
    "activeTab",
    "identity"
  ],
  "host_permissions": [
    "https://trello.com/*"
  ],
  "content_scripts": [
    {
      "js": [
        "assets/content-BQkop78M.js"
      ],
      "matches": [
        "https://trello.com/*"
      ]
    }
  ],
  "background": {
    "service_worker": "service-worker-loader.js",
    "type": "module"
  },
  "web_accessible_resources": [
    {
      "matches": [
        "https://trello.com/*"
      ],
      "resources": [
        "assets/content-BQkop78M.js"
      ],
      "use_dynamic_url": false
    }
  ]
}
